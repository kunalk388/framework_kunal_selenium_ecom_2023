<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="4" parallel="tests" verbose="4">


	<listeners>
		<listener class-name="com.kunal.opencart.listners.ExtentReportListener"></listener>

		<listener class-name="com.kunal.opencart.listners.TestAllureListener"></listener>

		<listener class-name="com.kunal.opencart.listners.AnnotationTransformer"></listener>

	</listeners>

	<test name="Login Page Test_chrome_90">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="90.0" />
		<parameter name="testcasename" value="Login Page Test" />
		<classes>
			<class name="com.kunal.opencart.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Accounts Page Test_chrome_79">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="79.0" />
		<parameter name="testcasename" value="Accounts Page Test" />
		<classes>
			<class name="com.kunal.opencart.tests.AccountsPageTest" />
		</classes>
	</test>

	<test name="Accounts Page Test_chrome_109">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="109.0" />
		<parameter name="testcasename" value="Accounts Page Test" />
		<classes>
			<class name="com.kunal.opencart.tests.AccountsPageTest" />
		</classes>
	</test>
